  "devDependencies": {
    "@types/bun": "^1.3.5",
    "@vitest/coverage-v8": "^4.0.16",
    "bun-types": "^1.1.4",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vitest": "^4.0.16"
  },
  "module": "index.ts"
}